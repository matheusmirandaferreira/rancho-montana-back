{
  "name": "typeorm-new-version",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "type": "commonjs",
  "scripts": {
    "start": "env-cmd -f .env.$npm_config_env ts-node-dev src/server.ts --watch",
    "typeorm": "env-cmd -f .env.$npm_config_env typeorm-ts-node-commonjs",
    "seed": "env-cmd -f .env.$npm_config_env ts-node-dev ./node_modules/typeorm-extension/dist/cli/index.js seed",
    "build": "tsc --outDir dist"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "email-validator": "^2.0.4",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "normalize-text": "^2.4.1",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.3.16",
    "typeorm-extension": "^2.8.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.10",
    "@types/pg": "^8.10.2",
    "@types/uuid": "^9.0.2",
    "ts-node": "10.7.0",
    "ts-node-dev": "^2.0.0",
    "tsup": "^7.2.0",
    "typescript": "^5.1.3"
  }
}
